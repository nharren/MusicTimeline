<Window x:Class="NathanHarrenstein.MusicTimeline.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Name="window"
        Title="Composer Timeline">
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="File">
                <MenuItem Command="{Binding ManageDataCommand, ElementName=window}" Header="Manage Data" />
                <Separator />
                <MenuItem Command="{Binding CloseCommand, ElementName=window}" Header="Close" />
            </MenuItem>
        </Menu>
        <Frame Name="Frame"
               NavigationUIVisibility="Hidden"
               Source="TimelinePage.xaml" />
    </DockPanel>
</Window>