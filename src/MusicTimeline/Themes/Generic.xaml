                                    VerticalAlignment="Top"
                                    Cursor="Hand"
                                    Visibility="Collapsed">
                                <Button.Template>
                                    <ControlTemplate TargetType="{x:Type Button}">
                                        <Border Name="border"
                                                Background="Transparent"
                                                BorderThickness="0"
                                                SnapsToDevicePixels="True">
                                            <Viewbox Height="14"
                                                     Margin="15"
                                                     Stretch="Uniform">
                                                <Path Fill="{DynamicResource HeaderBrush}">
                                                    <Path.Data>
