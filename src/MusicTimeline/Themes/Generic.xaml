                                           Height="22"
                                           Duration="5"
                                           FinalPause="0.5"
                                           FontFamily="Segoe UI Semibold"
                                           FontSize="16"
                                           Foreground="White"
                                           InitialPause="2" />
                                <TextBlock Name="nowPlayingArtistTextBlock"
                                           VerticalAlignment="Top"
                                           FontFamily="Segoe UI"
                                           FontSize="12"
                                           Foreground="#FFBBBBBB" />
                            </StackPanel>
