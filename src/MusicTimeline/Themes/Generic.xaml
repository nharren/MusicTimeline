                                                    </Path.Data>
                                                </Path>
                                            </Viewbox>
                                        </Border>
                                    </ControlTemplate>
                                </Button.Template>
                            </Button>
                            <ListBox Name="listBox"
                                     VerticalAlignment="Bottom"
                                     SelectedIndex="0">
                                <ListBox.Template>
                                    <ControlTemplate TargetType="{x:Type ListBox}">
                                        <c:PanningScrollViewer VerticalScrollBarVisibility="Disabled">
                                            <ItemsPresenter />
                                        </c:PanningScrollViewer>
                                    </ControlTemplate>
                                </ListBox.Template>
                                <ListBox.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <StackPanel Height="30" Orientation="Horizontal" />
                                    </ItemsPanelTemplate>
                                </ListBox.ItemsPanel>
                                <ListBox.ItemContainerStyle>
                                    <Style TargetType="ListBoxItem">
                                        <Setter Property="Padding" Value="0" />
                                        <Setter Property="BorderThickness" Value="0" />
                                        <Setter Property="Cursor" Value="Hand" />
                                    </Style>
                                </ListBox.ItemContainerStyle>
                            </ListBox>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--#endregion ImagesPanel-->

    <!--#region Marquee-->
    <Style TargetType="c:Marquee">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="c:Marquee">
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                        <ScrollViewer Width="Auto"
                                      Height="Auto"
                                      Margin="0"
                                      HorizontalContentAlignment="Stretch"
                                      VerticalContentAlignment="Stretch"
                                      BorderThickness="0"
                                      Padding="0"
                                      VerticalScrollBarVisibility="Disabled">
                            <Canvas>
                                <ContentPresenter x:Name="PART_Content"
                                                  HorizontalAlignment="Center"
                                                  VerticalAlignment="Center" />
                            </Canvas>
                        </ScrollViewer>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--#endregion Marquee-->

    <!--#region Mp3PlayerControl-->
