{
  "version": "7.0.0-*",
  "description": "Command line utilities for Entity Framework.",
  "compilationOptions": {
    "warningsAsErrors": true
  },
  "dependencies": {
    "EntityFramework.Commands": "7.0.0-beta4-12943"
  },
  "compile": "..\\Shared\\*.cs",
  "exclude": "tools\\Handlers.cs",
  "frameworks": {
    "net451": {},
    "dnx451": {
      "dependencies": {
        "Microsoft.AspNet.Hosting": "1.0.0-*",
        "Microsoft.AspNet.Hosting.Interfaces": "1.0.0-*",
        "Microsoft.Framework.CommandLineUtils": {
          "version": "1.0.0-*",
          "type": "build"
        }
      }
    },
    "dnxcore50": {
      "dependencies": {
        "Microsoft.AspNet.Hosting": "1.0.0-*",
        "Microsoft.AspNet.Hosting.Interfaces": "1.0.0-*",
        "Microsoft.Framework.CommandLineUtils": {
          "version": "1.0.0-*",
          "type": "build"
        }
      }
    }
  },
  "commands": {
    "ef": "EntityFramework.Commands"
  },
  "entryPoint": "EntityFramework.Commands",
  "loadable": false
}